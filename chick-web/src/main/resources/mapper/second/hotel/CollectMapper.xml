<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chick.second.hotel.mapper.CollectMapper">
    <select id="getCollect" resultType="com.chick.second.hotel.vo.HotelCollectionVO" parameterType="string">
        select
            *
        from
            collect c left join hotel h on c.hotel_id = h.id

    </select>
</mapper>
